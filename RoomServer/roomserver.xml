<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<connectionStrings>
		<!-- Example connection to a SQL Server Database on localhost. -->
		<!-- <add name="ExampleConnectionString"
			connectionString="Data Source=.;Initial Catalog=DBName;Integrated Security=True"
			providerName="System.Data.SqlClient" /> -->
	</connectionStrings>
	<appSettings>
		<!-- access these values via the property:
			System.Configuration.ConfigurationManager.AppSettings[key]
		-->
		<!-- 3的 倍数-->
		<add key="timeout" value="10"/>
		<add key="port" value="18910"/>
		<add key="apiport" value="10001"/>
		<add key="gameserver" value="GameServer.exe"/>
		<!-- 数组,隔开-->
		<add key="config" value="config.txt"/>
		<add key="ports" value="8910,8911,8912,8913,8914"/>
	</appSettings>
</configuration>
